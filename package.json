  "main": "index.js",
  "name": "fastify-backend",
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "build": "esbuild src/index.ts src/plugins/*.ts src/routes/**/*.ts --bundle --platform=node --target=node24 --outdir=dist --format=esm --packages=external --sourcemap",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org airbelgie-ji --project fastify-backend ./dist && sentry-cli sourcemaps upload --org airbelgie-ji --project fastify-backend ./dist",
    "test": "vitest",
    "watch": "concurrently --kill-others 'pnpm run watch:build' 'pnpm run watch:server'",
    "watch:build": "pnpm run build --watch",
    "watch:server": "NODE_ENV=local nodemon --env-file=.env dist/index.js"
